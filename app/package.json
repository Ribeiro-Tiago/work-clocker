{
	"name": "work-clocker",
	"version": "1.0.2",
	"description": "A \"card punch\" app to allow workers to better manage their time to avoid working more than the 8 hours they need to. Among other things, tells you when you can leave, taking into account the starting time, lunch duration and duration of a regular work day",
	"scripts": {
		"start": "ionic serve",
		"android": "ionic cordova run android -l",
		"android-prod": "npm run clean && ionic cordova build android --prod --release && cd platforms/android && ./gradlew bundleRelease",
		"ios": "ionic cordova run ios -l",
		"ios-prod": "npm clean clean && ionic cordova build ios --prod --release",
		"build": "npm run clean && ionic cordova build android --debug --verbose",
		"clean": "rm -rf platforms plugins www"
	},
	"private": false,
